{
  "presets": [["next/babel"]],
  "plugins": [
    "babel-plugin-macros",
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ],
    "inline-react-svg"
  ],
  "env": {
    "production": {
      "plugins": [
        ["styled-components", { "ssr": true, "displayName": false, "preprocess": false }]
      ]
    }
  },
  "ignore": ["node_modules"]
}
